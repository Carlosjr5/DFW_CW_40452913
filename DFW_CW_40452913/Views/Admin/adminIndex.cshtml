@{
<h2>Admin Dashboard</h2>

<h3>Members</h3>
<ul>
  @foreach (var member in Model.Members)
  {
     <li>@member.FullName</li>
  }
</ul>

<h3>Petitions</h3>

<ul>
  @foreach (var petition in Model.Petitions)
  {
     <li>@petition.Title - Created by: @petition.Creator.FullName</li>
  }
</ul>

}
